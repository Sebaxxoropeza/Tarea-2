#include <stdio.h>
int main() {
   int num1, num2;
  
   // Leer los dos números enteros
   printf("Ingresa el primer número entero: ");
   scanf("%d", &num1);
   printf("Ingresa el segundo número entero: ");
   scanf("%d", &num2);
  
   // Verificar si num2 es divisor de num1
   if (num1 % num2 == 0) {
       printf("%d es divisor de %d\n", num2, num1);
   } else if (num2 % num1 == 0) {
       printf("%d es divisor de %d\n", num1, num2);
   } else {
       printf("Ninguno es divisor del otro\n");
   }
  
   return 0;
}
